# min	hour	day	month	weekday	command
*/15	*	*	*	*	run-parts /etc/crond/periodic/15min
0	*	*	*	*	run-parts /etc/crond/periodic/hourly
0	2	*	*	*	run-parts /etc/crond/periodic/daily
0	3	*	*	6	run-parts /etc/crond/periodic/weekly
0	5	1	*	*	run-parts /etc/crond/periodic/monthly
